{
  "css": {
    "at-rules": {
      "charset": {
        "__compat": {
          "description": "<code>@charset</code>",
          "mdn_url": "https://developer.mozilla.org/docs/Web/CSS/@charset",
          "support": {
            "chrome": {
              "version_added": "2"
            },
            "chrome_android": {
              "version_added": "18"
            },
            "edge": {
              "version_added": "12"
            },
            "firefox": {
              "version_added": "1.5",
              "notes": "Firefox 1 supported an invalid syntax where the character encoding is not between single or double quotes."
            },
            "firefox_android": {
              "version_added": "4"
