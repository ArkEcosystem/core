    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/node-fetch": "^2.1.2",
    "@types/request": "^2.47.1",
    "@types/uuid": "^3.4.4",
    "codecov": "^3.1.0",
    "gts": "^0.9.0",
    "mocha": "^5.2.0",
