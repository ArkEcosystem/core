{
  "name": "snyk-gradle-plugin",
  "description": "Snyk CLI Gradle plugin",
  "homepage": "https://github.com/snyk/snyk-gradle-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/snyk-gradle-plugin"
  },
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist",
    "lib/init.gradle"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project tsconfig.json --format stylish",
    "prepare": "npm run build",
    "test": "npm run lint && tsc -p tsconfig-test.json && npm run test-functional && npm run test-system",
    "test-functional": "tap --node-arg=-r --node-arg=ts-node/register -R spec ./test/functional/*.test.[tj]s",
    "test-system": "tap --node-arg=-r --node-arg=ts-node/register -R spec --timeout=180 ./test/system/*.test.[tj]s",
    "test-manual": "jest test/manual -b"
  },
  "author": "snyk.io",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^4.9.1",
    "@types/sinon": "^7.0.10",
    "@types/tmp": "0.0.34",
    "jest": "^24.8.0",
    "sinon": "^2.4.1",
    "tap": "^12.6.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.14.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@snyk/cli-interface": "^2.1.0",
    "@types/debug": "^4.1.4",
    "chalk": "^2.4.2",
    "clone-deep": "^0.3.0",
    "debug": "^4.1.1",
    "tmp": "0.0.33",
    "tslib": "^1.9.3"
  },
  "version": "3.1.0"
}
