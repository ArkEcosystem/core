{
  "name": "snyk-policy",
  "description": "Snyk's policy parser and matching logic",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint -c .eslintrc lib",
    "check-tests": "! grep 'test.only' test/**/*.test.js -n",
    "env": "node -e 'console.log(process.env, process.versions)'",
    "tap": "tap test/**/*.test.js -R spec --timeout=60",
    "test": "npm run check-tests && npm run lint && npm run tap"
  },
  "keywords": [
    "snyk"
  ],
  "author": "Remy Sharp",
  "license": "Apache-2.0",
  "devDependencies": {
    "eslint": "^4.0.0",
    "proxyquire": "^2.1.0",
    "sinon": "^4.0.0",
    "tap": "^12.0.1",
    "tap-only": "0.0.5"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "email-validator": "^2.0.4",
    "js-yaml": "^3.13.1",
    "lodash.clonedeep": "^4.5.0",
    "semver": "^6.0.0",
    "snyk-module": "^1.9.1",
    "snyk-resolve": "^1.0.1",
    "snyk-try-require": "^1.3.1",
    "then-fs": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/policy.git"
  },
  "version": "1.13.5"
}
