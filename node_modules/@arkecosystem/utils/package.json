{
    "name": "@arkecosystem/utils",
    "description": "Performance oriented implementations of commonly used functions in TypeScript.",
    "version": "1.2.0",
    "contributors": [
        "Brian Faust <brian@ark.io>",
        "Joshua Noack <joshua@ark.io>"
    ],
    "license": "MIT",
    "main": "dist/index",
    "types": "dist/index",
    "files": [
        "dist"
    ],
    "scripts": {
        "prepublishOnly": "yarn build",
        "pretest": "yarn lint && yarn build",
        "compile": "tsc",
        "build": "yarn clean && yarn compile",
        "build:watch": "yarn clean && yarn compile -w",
        "clean": "del dist",
        "format": "yarn lint && yarn prettier",
        "lint": "eslint src --ext .ts --fix",
        "prettier": "prettier --write \"./*.{ts,js,json,md}\" \"./src/**/*.{ts,js,json,md}\"",
        "bench": "yarn build && node benchmark/index.js",
        "test": "jest",
        "coverage:report": "codecov"
    },
    "dependencies": {
        "@hapi/bourne": "^2.0.0",
        "deepmerge": "^4.2.2",
        "fast-copy": "^2.1.0",
        "fast-deep-equal": "^3.1.3",
        "fast-sort": "^2.2.0",
        "type-fest": "^0.16.0"
    },
    "devDependencies": {
        "@babel/core": "^7.11.0",
        "@babel/preset-env": "^7.11.0",
        "@faustbrian/benchmarker": "^0.1.2",
        "@hapi/boom": "^9.1.0",
        "@hapi/hapi": "^19.2.0",
        "@sindresorhus/tsconfig": "^0.7.0",
        "@types/benchmark": "^1.0.33",
        "@types/jest": "^26.0.8",
        "@types/lodash": "^4.14.158",
        "@types/node": "^14.0.27",
        "@types/prettier": "^2.0.2",
        "@typescript-eslint/eslint-plugin": "^3.7.1",
        "@typescript-eslint/parser": "^3.7.1",
        "@yarnpkg/pnpify": "^2.1.0",
        "benchmark": "^2.1.4",
        "big-number": "^2.0.0",
        "big.js": "^5.2.2",
        "bignumber.js": "^9.0.0",
        "codecov": "^3.7.2",
        "cross-env": "^7.0.2",
        "del-cli": "^3.0.1",
        "eslint": "^7.6.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-jest": "^23.20.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-simple-import-sort": "^5.0.3",
        "husky": "^4.2.5",
        "hyperid": "^2.0.5",
        "jest": "^26.2.2",
        "jest-extended": "^0.11.5",
        "jsbi": "^3.1.3",
        "lint-staged": "^10.2.11",
        "lodash": "^4.17.19",
        "pluralize": "^8.0.0",
        "prettier": "^2.0.5",
        "random-object": "^1.3.4",
        "ts-jest": "^26.1.4",
        "typescript": "^3.9.7",
        "typescript-language-server": "^0.4.0"
    },
    "engines": {
        "node": ">=10.x"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}
