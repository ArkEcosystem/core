{
    "name": "@arkecosystem/core-manager",
    "version": "3.0.0-alpha.9",
    "description": "Manager for ARK Core",
    "license": "MIT",
    "contributors": [
        "Sebastijan Kuzner <sebastijan@ark.io>"
    ],
    "files": [
        "dist"
    ],
    "main": "dist/index",
    "types": "dist/index",
    "scripts": {
        "build": "yarn run clean && yarn run compile",
        "build:watch": "yarn run clean && yarn run compile -w",
        "build:docs": "typedoc --out docs src",
        "clean": "del dist",
        "compile": "tsc",
        "prepublishOnly": "yarn run build",
        "pretest": "bash ../../scripts/pre-test.sh"
    },
    "dependencies": {
        "@arkecosystem/core-cli": "workspace:packages/core-cli",
        "@arkecosystem/core-database": "workspace:packages/core-database",
        "@arkecosystem/core-kernel": "workspace:packages/core-kernel",
        "@arkecosystem/core-state": "workspace:packages/core-state",
        "@arkecosystem/crypto": "workspace:packages/crypto",
        "@arkecosystem/utils": "^1.2.0",
        "@hapi/basic": "^6.0.0",
        "@hapi/boom": "^9.0.0",
        "@hapi/hapi": "^19.0.0",
        "@hapi/joi": "^17.1.1",
        "@hapist/json-rpc": "^0.2.0",
        "@hapist/whitelist": "^0.1.0",
        "@sindresorhus/df": "^3.1.1",
        "argon2": "^0.26.2",
        "better-sqlite3": "^7.0.0",
        "execa": "^3.4.0",
        "fs-extra": "^8.1.0",
        "got": "^11.1.3",
        "hapi-auth-bearer-token": "^6.1.6",
        "latest-version": "^5.1.0",
        "require-from-string": "^2.0.2",
        "typeorm": "^0.2.25"
    },
    "devDependencies": {
        "nock": "^13.0.0"
    },
    "engines": {
        "node": ">=10.x"
    },
    "publishConfig": {
        "access": "public"
    },
    "arkecosystem": {
        "core": {
            "alias": "manager"
        }
    }
}
