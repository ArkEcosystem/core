{
    "core": {
        "plugins": [
            {
                "package": "@arkecosystem/core-logger-pino"
            },
            {
                "package": "@arkecosystem/core-state"
            },
            {
                "package": "@arkecosystem/core-database"
            },
            {
                "package": "@arkecosystem/core-transactions"
            },
            {
                "package": "@arkecosystem/core-magistrate-transactions"
            },
            {
                "package": "@arkecosystem/core-transaction-pool"
            },
            {
                "package": "@arkecosystem/core-p2p",
                "options": {
                    "minimumNetworkReach": 1
                }
            },
            {
                "package": "@arkecosystem/core-blockchain"
            },
            {
                "package": "@arkecosystem/core-api"
            },
            {
                "package": "@arkecosystem/core-magistrate-api"
            },
            {
                "package": "@arkecosystem/core-webhooks"
            },
            {
                "package": "@arkecosystem/core-forger"
            },
            {
                "package": "@foly/socket-event-forwarder",
                "options": {
                    "enabled": false,
                    "port": 3333,
                    "events": [
                        "shutdown",
                        "block.applied",
                        "block.disregarded",
                        "block.forged",
                        "block.received",
                        "block.reverted",
                        "delegate.registered",
                        "delegate.resigned",
                        "forger.failed",
                        "forger.missing",
                        "forger.started",
                        "internal.milestone.changed",
                        "peer.added",
                        "peer.removed",
                        "round.applied",
                        "round.created",
                        "round.missed",
                        "snapshot.start",
                        "snapshot.progress",
                        "snapshot.complete",
                        "stateBuilder.finished",
                        "state.starting",
                        "state.started",
                        "transaction.applied",
                        "transaction.expired",
                        "transaction.forged",
                        "transaction.reverted",
                        "wallet.vote",
                        "wallet.unvote",
                        "scooter.registered",
                        "scooter.rental.start",
                        "scooter.rental.finish",
                        "wallet.logged.in",
                        "wallet.logged.out",
                        "character.registered"
                    ],
                    "confirmations": [
                        1,
                        2,
                        3,
                        4,
                        5
                    ],
                    "customEvents": [
                        "transaction.confirmed",
                        "systeminformation",
                        "network.latency",
                        "blockheight.current"
                    ],
                    "systeminformationInterval": 5000,
                    "networkLatencyInterval": 10000,
                    "blockheightCurrentInterval": 5000
                }
            },
            {
                "package": "@deadlock-delegate/mqtt",
                "options": {
                    "enabled": false,
                    "events": [
                        "block.forged",
                        "block.applied",
                        "transaction.applied",
                        "scooter.registered",
                        "scooter.rental.start",
                        "scooter.rental.finish",
                        "wallet.logged.in",
                        "wallet.logged.out",
                        "character.registered"
                    ],
                    "topic": "radians/events",
                    "mqttBroker": ""
                }
            }
        ]
    },
    "relay": {
        "plugins": [
            {
                "package": "@arkecosystem/core-logger-pino"
            },
            {
                "package": "@arkecosystem/core-state"
