            {
                "package": "@arkecosystem/core-magistrate-api"
            },
            {
                "package": "@arkecosystem/core-webhooks"
            },
            {
                "package": "@foly/socket-event-forwarder",
                "options": {
                    "enabled": false,
                    "port": 3333,
                    "events": [
                        "shutdown",
