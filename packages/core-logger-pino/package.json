{
    "name": "@arkecosystem/core-logger-pino",
    "version": "4.0.0-next.12",
    "description": "Webhooks for ARK Core",
    "license": "MIT",
    "contributors": [
        "Brian Faust <brian@ark.io>"
    ],
    "files": [
        "dist"
    ],
    "main": "dist/index",
    "scripts": {
        "build": "pnpm clean && pnpm compile",
        "build:watch": "pnpm clean && pnpm compile -w",
        "build:docs": "../../node_modules/typedoc/bin/typedoc --out docs src",
        "clean": "rimraf dist",
        "compile": "node ../../node_modules/typescript/bin/tsc",
        "prepublishOnly": "pnpm build",
        "pretest": "bash ../../scripts/pre-test.sh",
        "publish:next": "pnpm publish --access public --tag next --git-checks=false",
        "publish:latest": "pnpm publish --access public --git-checks=false"
    },
    "dependencies": {
        "@arkecosystem/core-kernel": "workspace:*",
        "chalk": "4.1.2",
        "joi": "17.4.2",
        "pino": "6.13.2",
        "pino-pretty": "4.8.0",
        "pump": "3.0.0",
        "pumpify": "2.0.1",
        "readable-stream": "3.6.0",
        "rotating-file-stream": "2.1.5",
        "split2": "3.2.2"
    },
    "engines": {
        "node": ">=10.x"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@types/pino": "6.3.11",
        "@types/pump": "1.1.1",
        "@types/readable-stream": "2.3.11",
        "@types/split2": "2.1.6"
    }
}
