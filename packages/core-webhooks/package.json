{
    "name": "@arkecosystem/core-webhooks",
    "version": "3.0.0-next.0",
    "description": "Webhooks for ARK Core",
    "license": "MIT",
    "contributors": [
        "Brian Faust <brian@ark.io>"
    ],
    "files": [
        "dist"
    ],
    "main": "dist/index",
    "scripts": {
        "build": "yarn clean && yarn compile",
        "build:watch": "yarn clean && yarn compile -w",
