    "fs-extra": "^6.0.1",
    "ioredis": "^3.2.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "mysql2": "^1.5.3",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.8",
    "sqlite3": "^4.0.0",
    "tiny-glob": "^0.2.1",
    "umzug": "^2.1.0"
