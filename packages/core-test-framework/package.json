{
    "name": "@arkecosystem/core-test-framework",
    "version": "3.0.0-alpha.9",
    "description": "Test Framework for ARK Core",
    "license": "MIT",
    "contributors": [
        "Brian Faust <brian@ark.io>",
        "Erwann Gentric <erwann@ark.io>",
        "Joshua Noack <joshua@ark.io>"
    ],
    "files": [
        "dist"
    ],
    "main": "dist/index",
    "scripts": {
        "build": "yarn clean && yarn compile",
        "build:watch": "yarn clean && yarn compile -w",
        "build:docs": "../../node_modules/typedoc/bin/typedoc --out docs src",
        "clean": "del dist",
        "compile": "../../node_modules/typescript/bin/tsc",
        "prepublishOnly": "yarn build"
    },
    "dependencies": {
        "@arkecosystem/core": "workspace:packages/core",
        "@arkecosystem/core-api": "workspace:packages/core-api",
        "@arkecosystem/core-blockchain": "workspace:packages/core-blockchain",
        "@arkecosystem/core-cli": "workspace:packages/core-cli",
        "@arkecosystem/core-database": "workspace:packages/core-database",
        "@arkecosystem/core-forger": "workspace:packages/core-forger",
        "@arkecosystem/core-kernel": "workspace:packages/core-kernel",
        "@arkecosystem/core-magistrate-crypto": "workspace:packages/core-magistrate-crypto",
        "@arkecosystem/core-p2p": "workspace:packages/core-p2p",
        "@arkecosystem/core-state": "workspace:packages/core-state",
        "@arkecosystem/crypto": "workspace:packages/crypto",
        "bip39": "^3.0.2",
        "bs58": "^4.0.1",
        "bytebuffer": "^5.0.1",
        "chance": "^1.1.3",
        "env-paths": "^2.2.0",
        "fast-memoize": "^2.5.1",
        "fs-extra": "^8.1.0",
        "got": "^11.0.0",
        "jest-extended": "^0.11.2",
        "lodash.clonedeep": "^4.5.0",
        "tmp": "^0.2.0",
        "xstate": "^4.8.0"
    },
    "devDependencies": {
        "@types/got": "^9.6.9"
    },
    "engines": {
        "node": ">=10.x"
    },
    "publishConfig": {
        "access": "public"
    }
}
