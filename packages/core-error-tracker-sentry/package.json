{
  "name": "@arkecosystem/core-error-tracker-sentry",
  "description": "Sentry error tracker integration for Ark Core.",
  "version": "0.1.0",
  "contributors": [
    "Brian Faust <brian@ark.io>"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "yarn test && yarn build",
    "pretest": "yarn lint && yarn build",
    "build": "yarn clean && ../../node_modules/typescript/bin/tsc",
    "build:watch": "yarn clean && ../../node_modules/typescript/bin/tsc -w",
    "clean": "del dist",
    "docs": "../../node_modules/typedoc/bin/typedoc src --out docs",
    "lint": "tslint -c ../../tslint.json '**/*.ts' --fix"
  },
  "dependencies": {
    "@sentry/node": "^4.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10.x"
  }
}
