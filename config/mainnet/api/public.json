{
  "mount": true,
  "port": 4003,
  "versions": {
    "default": 2,
    "valid": [
      1,
      2
    ]
  },
  "cache": {
    "enabled": false,
    "options": {
      "engine": "catbox-redis",
      "host": "127.0.0.1",
      "expiresIn": 60000
    }
  },
  "rateLimit": {
    "enabled": false,
    "limit": 300,
    "expires": 60000
