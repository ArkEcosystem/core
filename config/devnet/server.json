{
  "port": 4002,
  "version": "2.0.0",
  "fastRebuild": false,
  "logging": {
    "file": "debug",
    "console": "debug"
  },
  "db": {
    "driver": "app/database/knex",
    "options": {
      "client": "sqlite3",
      "useNullAsDefault": true,
      "connection": {
        "filename": "storage/database/devnet.sqlite"
      }
    }
  },
  "queue": {
    "host": "localhost",
    "port": 6379
  },
  "peers": {
    "minimumNetworkReach": 10,
    "blackList": []
  }
}
